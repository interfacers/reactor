{
  "name": "@interfacers/reactor",
  "version": "0.1.23",
  "engines": {
    "node": ">=10"
  },
  "source": "src/index.tsx",
  "main": "dist/reactor.js",
  "umd:main": "dist/reactor.umd.js",
  "module": "dist/reactor.module.js",
  "esmodule": "dist/reactor.modern.js",
  "unpkg": "dist/reactor.umd.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "github:interfacers/reactor"
  },
  "prepare": "npm run build",
  "prepublish": "npm run build",
  "scripts": {
    "build": "microbundle --jsx React.createElement",
    "dev": "microbundle watch --jsx React.createElement",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint --cache --fix --ext .js,.ts,.tsx .",
    "lint:css": "stylelint './src/**/*.{js,jsx,ts,tsx,css,scss}' --cache",
    "format": "prettier --write './src/**/*.{js,jsx,ts,tsx,css,md,json}'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "styled-components": ">=5.2.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@n8tb1t/use-scroll-position": "^1.0.47",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.49",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-loader": "^8.1.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "microbundle": "^0.12.3",
    "prettier": "^2.1.1",
    "react": "16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-spring": "^8.0.27",
    "react-use-measure": "^2.0.1",
    "styled-components": "^5.2.0",
    "styled-icons": "^10.20.0",
    "stylelint": "^13.7.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.0.2",
    "use-onclickoutside": "^0.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix --max-warnings 0",
      "stylelint --cache"
    ],
    "*.{css,scss}": [
      "prettier --write",
      "stylelint --cache --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
